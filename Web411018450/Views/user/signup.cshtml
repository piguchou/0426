
@{
    ViewBag.Title = "signup";
}

<h2>signup</h2>
@using (Html.BeginForm("signup", "user", FormMethod.Post))
{
    <p>姓名:@Html.TextBox("name")@ViewBag.NameMessage</p>
    <p>帳號:@Html.TextBox("account")@ViewBag.accountMessage</p>
    <p>密碼:@Html.TextBox("password")@ViewBag.passwordMessage</p>
    <p>@ViewBag.Message</p>
    <input type="submit" value="註冊" />
}



